{
   "id":"com.dtt.amgauge",
   "version":"1.0.0",
   "name":"AM Gauge",
   "description":"AM Gauge Chart",
   "newInstancePrefix":"Amgauge",
   "icon":"data:image/png;base64,",
   "vendor":"AMCHARTS",
   "eula":"",
   "license":"",
   "webcomponents":[
      {
		 "kind":"main",
		 "tag":"com-dtt-amgauge",
		 "url":"https://fsptest.deloitteresources.com/com.sap.portal.resourcerepository/repo/com.dtt.FLP_Extension/amchart/AMGauge.js",
		 "integrity":"",
		 "ignoreIntegrity":true
      },
	  {
		"kind": "builder",
		"tag": "com-dtt-amgauge-bps",
		"url": "https://fsptest.deloitteresources.com/com.sap.portal.resourcerepository/repo/com.dtt.FLP_Extension/amchart/AMGauge_Builder.js",
		"integrity": "",
		"ignoreIntegrity": true
	  }
   ],
   "properties":{
      "PrimValue":{
         "type":"number",
         "description":"Primary value to be shown",
         "default":55
      },
      "SecValue":{
         "type":"number",
         "description":"Secondary value to be shown",
         "default":45
      },
	  "BottomText":{
         "type":"string",
         "description":"Text below the gauge",
         "default":""
      },
	  "GaugeScaleStart":{
         "type":"integer",
         "description":"Start of value of gauge scale",
         "default":0
      },
	  "GaugeScaleEnd":{
         "type":"integer",
         "description":"End of value of gauge scale",
         "default":100
      },
	  "Rng1Start":{
         "type":"integer",
         "description":"Start of value of gauge band 1",
         "default":0
      },
	  "Rng1End":{
         "type":"integer",
         "description":"End of value of gauge band 1",
         "default":45
      },
	  "Rng2Label":{
         "type":"string",
         "description":"Text to be shown on hover of band 1",
         "default":""
      },
	  "Rng2Start":{
         "type":"integer",
         "description":"Start of value of gauge band 2",
         "default":45
      },
	  "Rng2End":{
         "type":"integer",
         "description":"End of value of gauge band 2",
         "default":75
      },
	  "Rng3Label":{
         "type":"string",
         "description":"Text to be shown on hover of band 2",
         "default":""
      },
	  "Rng3Start":{
         "type":"integer",
         "description":"Start of value of gauge band 3",
         "default":75
      },
	  "Rng3End":{
         "type":"integer",
         "description":"End of value of gauge band 3",
         "default":100
      },
	  "Rng1Col":{
         "type":"string",
         "description":"Color value gauge band 1",
         "default":"#DA291C"
      },
	  "Rng2Col":{
         "type":"string",
         "description":"Color value gauge band 2",
         "default":"#FFCD00"
      },
	  "Rng3Col":{
         "type":"string",
         "description":"Color value gauge band 3",
         "default":"#43B02A"
      },
	  "PrimCol":{
         "type":"string",
         "description":"Color value of the secondary marker",
         "default":"#FFFFFF"
      },
	  "SecCol":{
         "type":"string",
         "description":"Color value of the primary marker",
         "default":"#FFFF00"
      },
	  "StAngle":{
         "type":"number",
         "description":"Start angle of the gauge",
         "default":-180
      },
	  "EndAngle":{
         "type":"number",
         "description":"End angle of the gauge",
         "default":0
      },
	  "PrimShow":{
         "type":"boolean",
         "description":"Show / Hide the primary hand",
         "default":true
      },
	  "SecShow":{
         "type":"boolean",
         "description":"Show / Hide the secondary hand",
         "default":true
      }
   },
   "methods":{
		"setPrimaryValue": {
			"description": "Sets the primary value (large arrow) of the gauge.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "The new gauge value."
				}
			],
			"body": "this.PrimValue = newValue;"
		},
		"setSecondaryValue": {
			"description": "Sets the secondary value (small arrow) of the gauge.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "The new gauge value."
				}
			],
			"body": "this.SecValue = newValue;"
		},
		"setChartValues": {
			"description": "Sets all the chart values. primval, secvalue, bottomtext, gaugescale, gaugerng1, gaugerng2, gaugerng3", 
			"parameters": [
				{
					"name": "newPrimValue",
					"type": "number",
					"description": "The new primary value."
				},
				{
					"name": "newSecValue",
					"type": "number",
					"description": "The new secondary value."
				},
				{
					"name": "newBottomTxt",
					"type": "string",
					"description": "The new bottom text."
				},
				{
					"name": "newGaugeStartValue",
					"type": "integer",
					"description": "The new start value of the gauge scale."
				},
				{
					"name": "newGaugeEndValue",
					"type": "integer",
					"description": "The new end value of the gauge scale."
				},
				{
					"name": "newRng1StartValue",
					"type": "integer",
					"description": "The new range 1 start value."
				},
				{
					"name": "newRng1EndValue",
					"type": "integer",
					"description": "The new range 1 end value."
				},
				{
					"name": "newRng2Label",
					"type": "string",
					"description": "The new range 2 Label."
				},
				{
					"name": "newRng2StartValue",
					"type": "integer",
					"description": "The new range 2 start value."
				},
				{
					"name": "newRng2EndValue",
					"type": "integer",
					"description": "The new range 2 end value."
				},
				{
					"name": "newRng3Label",
					"type": "string",
					"description": "The new range 3 Label."
				},
				{
					"name": "newRng3StartValue",
					"type": "integer",
					"description": "The new range 3 start value."
				},
				{
					"name": "newRng3EndValue",
					"type": "integer",
					"description": "The new range 3 end value."
				}
			],
			"body": "this.PrimValue = newPrimValue; this.SecValue = newSecValue; this.BottomText = newBottomTxt; this.GaugeScaleStart = newGaugeStartValue; this.GaugeScaleEnd = newGaugeEndValue; this.Rng1Start = newRng1StartValue; this.Rng1End = newRng1EndValue; this.Rng2Label = newRng2Label; this.Rng2Start = newRng2StartValue; this.Rng2End = newRng2EndValue; this.Rng3Label = newRng3Label; this.Rng3Start = newRng3StartValue; this.Rng3End = newRng3EndValue;"
		},
		"setChartAngles": {
			"description": "Sets the angles for the chart",
			"parameters": [
				{
					"name": "newStAngle",
					"type": "number",
					"description": "The new start angle."
				},
				{
					"name": "newEndAngle",
					"type": "number",
					"description": "The new end angle."
				}
			],
			"body": "this.StAngle = newStAngle; this.EndAngle = newEndAngle;"
		},
		"getPrimaryValue": {
			"returnType": "number",
			"description": "Returns the gauge value",
			"body": "return this.PrimValue;"
		}
   },
   "events":{
      "onClick":{
         "description":"Called when the user clicks the amchart."
      }
   }
}